<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <!--康复师详情页  问诊评价的列表（Rlv）-->
    <ImageView
        android:id="@+id/iv_therapister_picture"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="13dp"
        android:background="@drawable/picture_bg"
        app:layout_constraintEnd_toStartOf="@+id/tv_therpisters_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher_round"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_therpisters_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:text="陈艳萍"
        android:textColor="#ff666666"
        android:textSize="15sp"
        app:layout_constraintStart_toEndOf="@+id/iv_therapister_picture"
        app:layout_constraintTop_toTopOf="@+id/iv_therapister_picture" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="3dp"
        android:text="2019.8.23"
        android:textColor="#ff999999"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_therpisters_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv_therpisters_name" />

    <RadioGroup
        android:id="@+id/radioGroup"
        android:layout_width="wrap_content"
        android:layout_height="15dp"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="7dp"
        app:layout_constraintBottom_toBottomOf="@+id/iv_therapister_picture"
        app:layout_constraintStart_toStartOf="@+id/tv_therpisters_name"
        app:layout_constraintTop_toBottomOf="@+id/tv_therpisters_name">

        <ImageView
            android:id="@+id/imageView23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@color/color_o" />

        <ImageView
            android:id="@+id/imageView24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@color/color_o" />
    </RadioGroup>

    <ImageView
        android:id="@+id/iv_start_one"
        android:layout_width="14dp"
        android:layout_height="13dp"
        app:layout_constraintBottom_toBottomOf="@+id/radioGroup"
        app:layout_constraintStart_toStartOf="@+id/radioGroup"
        app:layout_constraintTop_toTopOf="@+id/radioGroup"
        app:layout_constraintVertical_bias="0.503"
        app:srcCompat="@color/color_o" />

    <ImageView
        android:id="@+id/iv-start_two"
        android:layout_width="14dp"
        android:layout_height="13dp"
        android:layout_marginStart="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/radioGroup"
        app:layout_constraintEnd_toStartOf="@+id/iv-start_three"
        app:layout_constraintStart_toEndOf="@+id/iv_start_one"
        app:layout_constraintTop_toTopOf="@+id/radioGroup"
        app:layout_constraintVertical_bias="0.503"
        app:srcCompat="@color/color_o" />

    <ImageView
        android:id="@+id/iv-start_three"
        android:layout_width="14dp"
        android:layout_height="13dp"
        android:layout_marginStart="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/radioGroup"
        app:layout_constraintEnd_toStartOf="@+id/iv_start_four"
        app:layout_constraintStart_toEndOf="@+id/iv-start_two"
        app:layout_constraintTop_toTopOf="@+id/radioGroup"
        app:layout_constraintVertical_bias="0.502"
        app:srcCompat="@color/color_o" />

    <ImageView
        android:id="@+id/iv_start_four"
        android:layout_width="14dp"
        android:layout_height="13dp"
        android:layout_marginStart="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/radioGroup"
        app:layout_constraintEnd_toStartOf="@+id/iv_start_five"
        app:layout_constraintStart_toEndOf="@+id/iv-start_three"
        app:layout_constraintTop_toTopOf="@+id/radioGroup"
        app:layout_constraintVertical_bias="0.502"
        app:srcCompat="@color/color_o" />

    <ImageView
        android:id="@+id/iv_start_five"
        android:layout_width="14dp"
        android:layout_height="13dp"
        android:layout_marginStart="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/radioGroup"
        app:layout_constraintStart_toEndOf="@+id/iv_start_four"
        app:layout_constraintTop_toTopOf="@+id/radioGroup"
        app:layout_constraintVertical_bias="0.502"
        app:srcCompat="@color/color_o" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="1dp"
        android:text="“王嘉尔”医生"
        android:textColor="#ff666666"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/iv-start_three"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/tv_wenzhen-thinks"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="63dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="14dp"
        android:text="将拉力带套成圈,套在双大腿下段髌骨上缘处,双上力。上半身紧贴墙,挺胸收腹。"
        android:textColor="#ff000000"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>