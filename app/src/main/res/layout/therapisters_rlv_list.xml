<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tv_therapists_huoyuezhinum"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
<!--康复培训机构的列表展示-->

    <ImageView
        android:id="@+id/iv_therapists_picture"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintEnd_toStartOf="@+id/tv_tuijian"
        app:layout_constraintStart_toEndOf="@+id/iv_sangang"
        app:layout_constraintTop_toTopOf="@+id/iv_hurt"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/iv_hurt"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="31dp"
        app:layout_constraintStart_toEndOf="@+id/tv_therapisters_name"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="51dp"
        android:layout_height="19dp"
        android:layout_marginStart="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/button_onoff_indicator_on" />

    <ImageView
        android:id="@+id/iv_therapisters_picture"
        android:layout_width="43dp"
        android:layout_height="44dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="29dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher_round" />

    <ImageView
        android:id="@+id/iv_start_show"
        android:layout_width="37dp"
        android:layout_height="36dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="47dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_input_add" />

    <TextView
        android:id="@+id/tv_therapisters_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="250dp"
        android:layout_marginBottom="7dp"
        android:text="康复师名字页"
        android:textColor="#ff000000"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_hospital"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_hospital"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="57dp"
        android:text="北医三院"
        android:textColor="#ff666666"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="@+id/tv_therapisters_name"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_therapists_zixunnum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="咨询量123"
        android:textColor="#ff999999"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="@+id/tv_hospital"
        app:layout_constraintTop_toBottomOf="@+id/tv_hospital" />

    <TextView
        android:id="@+id/tv_therapistsers_guanzhunum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="关注量123"
        android:textColor="#ff999999"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_therapists_zixunnum"
        app:layout_constraintStart_toEndOf="@+id/tv_therapists_zixunnum"
        app:layout_constraintTop_toTopOf="@+id/tv_therapists_zixunnum" />

    <TextView
        android:id="@+id/tv_therpistser_huoyuenum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="9dp"
        android:text="活跃值123568"
        android:textColor="#ff999999"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_therapistsers_guanzhunum"
        app:layout_constraintStart_toEndOf="@+id/tv_therapistsers_guanzhunum"
        app:layout_constraintTop_toTopOf="@+id/tv_therapistsers_guanzhunum"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/iv_sangang"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="3dp"
        app:layout_constraintStart_toEndOf="@+id/iv_hurt"
        app:layout_constraintTop_toTopOf="@+id/iv_hurt"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/iv_start"
        android:layout_width="14dp"
        android:layout_height="13dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="54dp"
        android:layout_marginBottom="41dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.017"
        app:srcCompat="@color/colorPrimaryDark"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_start_conte"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="17dp"
        android:text="4.5分"
        android:textColor="#ff333333"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/iv_start"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_start"
        app:layout_constraintTop_toTopOf="@+id/iv_start" />

    <TextView
        android:id="@+id/tv_tuijian"
        android:layout_width="32dp"
        android:layout_height="18dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="144dp"
        android:text="推荐"
        android:gravity="center"
        android:background="@drawable/recommention_bg"
        android:textColor="#ffff804e"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/iv_therapists_picture"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>