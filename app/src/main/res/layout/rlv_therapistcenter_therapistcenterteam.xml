<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="116dp">

<!--康复师详情页  康复师团队的列表（Rlv）-->
    <ImageView
        android:id="@+id/iv_therapister_picture"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:layout_marginStart="51dp"
        android:background="@drawable/picture_bg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher_round"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_therpisters_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="2dp"
        android:text="王嘉尔"
        android:textColor="#ff000000"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/iv_therapister_picture"
        app:layout_constraintStart_toStartOf="@+id/iv_therapister_picture"
        app:layout_constraintTop_toBottomOf="@+id/iv_therapister_picture" />

    <ImageView
        android:id="@+id/iv_theraptiser_start"
        android:layout_width="32dp"
        android:layout_height="31dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="27dp"
        android:layout_marginBottom="2dp"
        app:layout_constraintBottom_toTopOf="@+id/tv_therpisters_name"
        app:layout_constraintStart_toStartOf="@+id/iv_therapister_picture"
        app:layout_constraintTop_toTopOf="@+id/iv_therapister_picture"
        app:srcCompat="@color/color_o" />
</androidx.constraintlayout.widget.ConstraintLayout>